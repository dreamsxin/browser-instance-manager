# 浏览器管理器配置
BROWSER_MANAGER_MAX_INSTANCES=10
BROWSER_MANAGER_DEFAULT_BROWSER=chromium
BROWSER_MANAGER_DEFAULT_MODE=launch
BROWSER_MANAGER_TIMEOUT=30000

# 日志配置
BROWSER_MANAGER_LOG_LEVEL=info
BROWSER_MANAGER_LOG_TO_FILE=false
BROWSER_MANAGER_LOG_FILE_PATH=./logs/browser-manager.log

# 性能配置
BROWSER_MANAGER_HEALTH_CHECK_INTERVAL=30000
BROWSER_MANAGER_MAX_PAGES_PER_BROWSER=20
BROWSER_MANAGER_MEMORY_LIMIT=1024

# 浏览器配置
BROWSER_HEADLESS=true
BROWSER_VIEWPORT_WIDTH=1920
BROWSER_VIEWPORT_HEIGHT=1080

# 开发环境配置
NODE_ENV=development